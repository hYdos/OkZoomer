{
  "schemaVersion": 1,
  "id": "okzoomer-sunset",
  "version": "${version}",
  "name": "Ok Zoomer (Migrated to Quilt)",
  "description": "The last Fabric-compatible version was 5.0.0-beta.6. From 5.0.0-beta.7 onwards, this mod will exclusively support the Quilt toolchain.",
  "authors": ["EnnuiL"],
  "contact": {
    "homepage": "https://modrinth.com/mod/ok-zoomer",
    "issues": "https://github.com/EnnuiL/OkZoomer/issues",
    "sources": "https://github.com/EnnuiL/OkZoomer"
  },
  "license": "MIT",
  "icon": "assets/ok_zoomer/icon_sunset.png",
  "environment": "client",
  "entrypoints": {
    "client_init": ["io.github.ennuil.okzoomer.OkZoomerClientMod"],
    "client_events": ["io.github.ennuil.okzoomer.events.LoadConfigEvent"],
    "modmenu": ["io.github.ennuil.okzoomer.config.modmenu.OkZoomerModMenuEntry"],
    "preLaunch": ["io.github.ennuil.okzoomer.MixinExtrasInitializer"]
  },
  "depends": {
    "fabricloader": ">=0.13.3",
    "java": ">=17"
  },
  "recommends": {
    "quilt_loader": "0.16.0"
  },
  "custom": {
    "platform_migration_warning": {
      "author": "ennuil",
      "migrated_since": "5.0.0-beta.7",
      "implements_self": true
    }
  }
}
