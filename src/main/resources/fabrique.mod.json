  "authors": ["EnnuiL"],
  "contact": {
    "homepage": "https://modrinth.com/mod/ok-zoomer",
    "issues": "https://github.com/EnnuiL/OkZoomer/issues",
    "sources": "https://github.com/EnnuiL/OkZoomer"
  },
  "license": "MIT",
  "icon": "assets/ok_zoomer/icon_sunset.png",
  "environment": "client",
  "depends": {
    "fabricloader": ">=0.13.3",
    "java": ">=17"
  },
  "recommends": {
    "quilt_loader": "0.16.0"
  },
  "mixins": ["ok_zoomer_pmw.mixins.json"],
  "custom": {
    "platform_migration_warning": {
      "name": "Ok Zoomer",
      "migrated_since": "5.0.0-beta.7",
      "author": "ennuil",
      "implements_self": true
